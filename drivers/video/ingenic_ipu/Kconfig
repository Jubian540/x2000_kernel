menuconfig JZ_IPU
	bool "JZ ipu unit"
	default n

config INGENIC_IPU
	tristate "Ingenic IPU Driver"
	depends on JZ_IPU
	default n
	help
	  Support for Ingenic ipu operations.

config INGENIC_DRAWBOX
	tristate "Ingenic draw box Driver"
	depends on JZ_IPU
	default n
	help
	  Support for Ingenic ipu operations.
